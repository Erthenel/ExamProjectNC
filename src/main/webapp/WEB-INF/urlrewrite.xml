<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>


    <rule>
        <from>^/PersonalOffice/teacher$</from>
        <to>/PersonalOfficeTeacher.html</to>
    </rule>

    <outbound-rule>
        <from>^/PersonalOfficeTeacher.html$</from>
        <to>/PersonalOffice/teacher</to>
    </outbound-rule>

    <rule>
        <from>^/PersonalOffice/student$</from>
        <to>/PersonalOfficeStudent.html</to>
    </rule>

    <outbound-rule>
        <from>^/PersonalOfficeStudent.html$</from>
        <to>/PersonalOffice/student</to>
    </outbound-rule>

</urlrewrite>