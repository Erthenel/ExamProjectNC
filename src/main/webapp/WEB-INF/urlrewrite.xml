<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>


    <rule>
        <from>^/PersonalOffice/account$</from>
        <to>/PersonalOffice.html</to>
    </rule>

    <outbound-rule>
        <from>^/PersonalOffice.html$</from>
        <to>/PersonalOffice/account</to>
    </outbound-rule>


    <rule>
        <from>^/Registration$</from>
        <to>/Registration.html</to>
    </rule>

    <outbound-rule>
        <from>^/Registration.html$</from>
        <to>/Registration</to>
    </outbound-rule>

    <rule>
        <from>^/login$</from>
        <to>/login.jsp</to>
    </rule>

    <outbound-rule>
        <from>^/login.jsp$</from>
        <to>/login</to>
    </outbound-rule>

    <rule>
        <from>^/login?error$</from>
        <to>/login.jsp</to>
    </rule>

    <outbound-rule>
        <from>^/login.jsp$</from>
        <to>/login?error</to>
    </outbound-rule>

</urlrewrite>